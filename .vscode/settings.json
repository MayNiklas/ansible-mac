{
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "files.associations": {
        // .vscode
        ".vscode/*.json": "jsonc",
        // Ansible
        "host_vars/*.yml": "jinja-yaml",
        "ansible.cfg": "ini",
        "requirements.yml": "pip-requirements",
        "site.yml": "ansible",
    },
}